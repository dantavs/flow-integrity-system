# Ambiente da aplicação: use somente "dev" ou "prod"
# - dev: base local de desenvolvimento (flow_integrity_commitments_dev)
# - prod: base local de produção (flow_integrity_commitments_prod)
NEXT_PUBLIC_APP_ENV=dev

# Flow Guardian (IA)
# - habilite em dev para testar os endpoints de IA
NEXT_PUBLIC_FLOW_GUARDIAN_ENABLED=false
FLOW_GUARDIAN_PROVIDER=openai
FLOW_GUARDIAN_TIMEOUT_MS=15000
FLOW_GUARDIAN_MAX_RETRIES=1
FLOW_GUARDIAN_MODEL_ADVISOR=gpt-4o-mini
FLOW_GUARDIAN_MODEL_GRAPH=gpt-4.1-mini
# Owners ignorados para alerta de saturação (separados por vírgula)
FLOW_GUARDIAN_OWNER_SATURATION_EXCLUDE=Tavares
# segredo server-side (nunca usar NEXT_PUBLIC_)
OPENAI_API_KEY=
